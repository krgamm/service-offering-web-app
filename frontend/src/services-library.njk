---
name: Services Library
layout: base.njk
---
<div class="container">
    <ul class="service-library-wrapper">
        {%- for service in services-%}
            <li class="service-library-card">
                <h2>{{service.title}}</h2>
                <p>{{service.description}} Lorem ipsum dolor sit amet consectetur adipisicing elit. Obcaecati
                numquam molestiae natus doloribus assumenda neque at ratione possimus laboriosam cumque.</p>
                <ul class="tag-wrapper">
                    <li><p>Type:</p><p>{{service.category.type}}</p></li>
                    <li><p>Process:</p><p>{{service.category.process}}</p></li>
                </ul>
            </li>
        {%- endfor -%}
    </ul>
</div>